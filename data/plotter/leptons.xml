<?xml version="1.0" encoding="ISO-8859-1"?>
<monitor> 

  <!-- Muon pt and eta distribution (After lepton + 3jets )-->
   <id tag="muCh_pt_m" doratio="1" logy="1" cmspublic="y" opt="e1" oflow="0" uflow="0" title="" ytitle="Events / 10 GeV" xtitle="p^{#mu}_{T} [GeV]" yrange="10,2000000"  xbin="8">
    <item dir="PFlow/RecoSteps/lep_tau/#geq 3 jet">pt_m_4</item>
  </id>
  <id tag="muCh_eta_m" doratio="1" logy="0" cmspublic="y" opt="e1" oflow="0" uflow="0" title="" ytitle="Events" xtitle="#eta^{#mu}" yrange="0,200000" xrange="-2.6,2.6" xbin="2">
    <item dir="PFlow/RecoSteps/lep_tau/#geq 3 jet">eta_m_4</item>
  </id>

  <!-- DeltaR between lepton and jet (After lepton + 3jets) -->
   <id tag="muCh_DR1_m_jet" doratio="1" logy="1" cmspublic="y" opt="e1" oflow="0" uflow="0" title="" ytitle="Events" xrange="0,6" xtitle="#Delta R (#mu, jet)" yrange="10,2000000"  >
    <item dir="PFlow/RecoSteps/lep_tau/#geq 3 jet">DR1_lepton_jet_4</item>
  </id>
   <id tag="muCh_DR2_m_jet" doratio="1" logy="1" cmspublic="y" opt="e1" oflow="0" uflow="0" title="" ytitle="Events" xrange="0,6" xtitle="#Delta R (#mu, jet)" yrange="10,2000000" >
    <item dir="PFlow/RecoSteps/lep_tau/#geq 3 jet">DR2_lepton_jet_4</item>
  </id>
   <id tag="muCh_DR3_m_jet" doratio="1" logy="1" cmspublic="y" opt="e1" oflow="0" uflow="0" title="" ytitle="Events" xrange="0,6" xtitle="#Delta R (#mu, jet)" yrange="10,2000000"  >
    <item dir="PFlow/RecoSteps/lep_tau/#geq 3 jet">DR3_lepton_jet_4</item>
  </id>
   <id tag="muCh_DRALL_m_jet" doratio="1" logy="1" cmspublic="y" opt="e1" oflow="0" uflow="0" title="" ytitle="Events" xrange="0,6" xtitle="#Delta R (#mu, jet)" yrange="10,2000000" >
    <item dir="PFlow/RecoSteps/lep_tau/#geq 3 jet">DRALL_lepton_jet_4</item>
  </id>


  <!-- tau pt and eta distrubition (After 1tau ) -->

  <id tag="muCh_pt_t_1t" doratio="1" logy="1" cmspublic="y" opt="e1" oflow="1" uflow="0" xbin="4" title="" ytitle="Events / 10 GeV"
  xtitle="p^{#tau}_{T} [GeV]" yrange="0.1,10000" xrange="20,500">
    <item dir="PFlow/RecoSteps/lep_tau/1 Tau">pt_t_8</item>
  </id> 

  <id tag="muCh_eta_t_1t" doratio="1" logy="0" cmspublic="y"  oflow="1" uflow="0" title="" ytitle="Events" xtitle="#eta^{#tau}" xbin="4" yrange="0,1500">
    <item dir="PFlow/RecoSteps/lep_tau/1 Tau">eta_t_8</item>
  </id> 

  <!-- tau pt and eta distrubition (After OS ) -->

  <id tag="muCh_pt_t_OS" doratio="1" logy="1" cmspublic="y" opt="e1" oflow="1" uflow="0" xbin="4" title="" ytitle="Events / 10 GeV"
  xtitle="p^{#tau}_{T} [GeV]" yrange="0.1,10000" xrange="20,350">
    <item dir="PFlow/RecoSteps/lep_tau/OS">pt_t_9</item>
  </id> 

  <id tag="muCh_eta_t_OS" doratio="1" logy="0" cmspublic="y"  oflow="1" uflow="0" title="" ytitle="Events" xtitle="#eta^{#tau}" xbin="1" yrange="0,400">
    <item dir="PFlow/RecoSteps/lep_tau/OS">eta_t_9</item>
  </id> 




<!-- R after OS -->
  <id tag="muCh_r_t_OS" doratio="0" logy="0" cmspublic="y"  oflow="1" uflow="0" title="" ytitle="a.u." xtitle="p^{lead.track}_{T}/|p^{#tau}_{T}|" norm="1" xbin="2" yrange="0,1">
    <item dir="PFlow/RecoSteps/lep_tau/OS">r_t_9</item>
  </id>	

  <id tag="muCh_rb_t_OS" doratio="0" logy="0" cmspublic="y"  oflow="1" uflow="0" title="" ytitle="a.u." xtitle="p^{lead.track}_{T}/p^{ass.jet}_{T}" norm="1" xbin="2" yrange="0,1">
    <item dir="PFlow/RecoSteps/lep_tau/OS">rb_t_9</item>
  </id> 



  <id tag="muCh_rc_t_OS" doratio="0" logy="0" cmspublic="y"  oflow="1" uflow="0" title="" ytitle="a.u." xtitle="p^{lead.track}/E^{#tau}" norm="1" xbin="2" yrange="0,0.4">
    <item dir="PFlow/RecoSteps/lep_tau/OS">rc_t_9</item>
  </id> 
  
</monitor>
